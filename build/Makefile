# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  HashTest_config = debug
endif
ifeq ($(config),release)
  HashTest_config = release
endif

PROJECTS := HashTest

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

HashTest:
ifneq (,$(HashTest_config))
	@echo "==== Building HashTest ($(HashTest_config)) ===="
	@${MAKE} --no-print-directory -C . -f HashTest.make config=$(HashTest_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f HashTest.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   HashTest"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"